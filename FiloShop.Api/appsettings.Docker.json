{
  "ConnectionStrings": {
    "DefaultConnection": "Host=filoshop-db;Port=5432;Database=filoshop_docker;User Id=postgres;Password=postgres",
    "Cache": "filoshop-redis:6379"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://filoshop-seq:5341"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "Authentication": {
    "Audience": "account",
    "Issuer": "http://filoshop-idp:8080/realms/filoshop",
    "MetaDataUrl": "http://filoshop-idp:8080/realms/filoshop/.well-known/openid-configuration",
    "RequireHttpsMetadata": false
  },
  "Keycloak": {
    "BaseUrl": "http://filoshop-idp:8080",
    "AdminUrl": "http://filoshop-idp:8080/admin/realms/filoshop/",
    "TokenUrl": "http://filoshop-idp:8080/realms/filoshop/protocol/openid-connect/token",
    "AdminClientId": "filoshop-admin-client",
    "AdminClientSecret": "WXfoHU7SoAVKhmwlkX16LopgvgJSGvDQ",
    "AuthClientId": "filoshop-auth-client",
    "AuthClientSecret": "DqZgQwcljcBlsVhFklWlAiNrnnv6dM4X"
  },
  "Outbox": {
    "IntervalInSeconds": 10,
    "BatchSize": 10
  }
}